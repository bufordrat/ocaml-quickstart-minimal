VCS = none

.PHONY: dirtycheck
dirtycheck::

.PHONY: versioncheck
versioncheck: dirtycheck

.PHONY: bump-major
bump-major::
	$(error no VCS here)

.PHONY: bump-minor
bump-minor::
	$(error no VCS here)

.PHONY: bump-patch
bump-patch::
	$(error no VCS here)
